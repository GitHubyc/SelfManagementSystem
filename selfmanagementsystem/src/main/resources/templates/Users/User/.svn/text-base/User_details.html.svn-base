<!DOCTYPE html>
<html lang="zh-CN">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,Chrome=1" />
<meta name="viewport"
	content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<title>用户信息</title>
<!--[if IE 7]>
    <link href="css/font-awesome-ie7.min.css" rel="stylesheet">
   <![endif]-->
<!-- Bootstrap -->
<link href="../../css/bootstrap.min.css" rel="stylesheet">
<!-- 字体图标 -->
<link href="../../css/font-awesome.min.css" rel="stylesheet">
<!-- NProgress -->
<link href="../../css/nprogress.css" rel="stylesheet">
<!-- 公共样式 -->
<link href="../../css/general.css" rel="stylesheet" media="screen">
<!-- 日期样式 -->
<link href="../../css/bootstrap-datepicker3.min.css" rel="stylesheet">
<!-- toastr -->
<link href="../../css/toastr.min.css" rel="stylesheet">
<link href="../../css/bootstrapValidator.min.css" rel="stylesheet">
<!-- 选框样式 -->
<link rel="stylesheet" type="text/css" href="../../css/build.css">
<link href="../../css/add.css" rel="stylesheet" media="screen">

</head>
<body class="bg_white">
	<div class="padding_15">
		<div class="row">
			<div class="col-md-12">
						<h4>查看用户</h4>
						<hr/>
					<div class="x_content">
						<form class="form-horizontal form_style" id="User" method="post">
							<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12"
								th:each="user:${object.data}">
								<div class="form_content">
									<h4 class="font_ioc_blue">
										<strong><i class="fa fa-gear"></i>用户类型</strong>
									</h4>
									<div class="row">
										<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">用户类型:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div th:switch="${user.userType}">
														<div th:case="0">
															<input class="form-control col-md-7 col-xs-12"
																name="userId" placeholder="用户ID" type="text"
																value="超级管理员" disabled="disabled">
														</div>
														<div th:case="1">
															<input class="form-control col-md-7 col-xs-12"
																name="userId" placeholder="用户ID" type="text"
																value="单位用户" disabled="disabled">
														</div>
														<div th:case="2">
															<input class="form-control col-md-7 col-xs-12"
																name="userId" placeholder="用户ID" type="text"
																value="单位员工用户" disabled="disabled">
														</div>
														<div th:case="3">
															<input class="form-control col-md-7 col-xs-12"
																name="userId" placeholder="用户ID" type="text"
																value="建筑用户" disabled="disabled">
														</div>
													</div>
												</div>
											</div>
										</div>
										<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
											<div class="item form-group has-feedback" style="display: none">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">用户ID:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="userId" placeholder="用户ID" type="text"
														th:value="${user.userId}" disabled="disabled">
												</div>
											</div>
										</div>
									</div>
									<div class="row">
										<div th:switch="${user.userType}">
											<div th:case="1">
												<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
													<div class="item form-group has-feedback">
														<label
															class="text-left col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">单位:
														</label>
														<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
															<select class="form-control" name="unitCode" disabled="disabled">
																<div th:each="unit:${units.data}">
																	<option th:value="${user.unitCode}" th:text="${unit.unitName}"
																		th:selected="${user.unitCode} == ${unit.unitCode}"></option>
																</div>
															</select>
														</div>
													</div>
												</div>
											</div>
											<div th:case="2">
												<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
													<div class="item form-group has-feedback">
														<label
															class="text-left col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">单位:
														</label>
														<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
															<select class="form-control" name="unitCode" disabled="disabled">
																<div th:each="unit:${units.data}">
																	<option th:value="${user.unitCode}" th:text="${unit.unitName}"
																		th:selected="${user.unitCode} == ${unit.unitCode}"></option>
																</div>
															</select>
														</div>
													</div>
												</div>
												<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
													<div class="item form-group has-feedback">
														<label
															class="text-left col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">员工:
														</label>
														<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
															<select class="form-control" name="unitCode" disabled="disabled">
																<div th:each="employee:${employees.data}">
																	<option th:value="${user.employeeCode}" th:text="${employee.name}"
																		th:selected="${user.employeeCode} == ${employee.employeeCode}"></option>
																</div>
															</select>
														</div>
													</div>
												</div>
											</div>
											<div th:case="3">
												<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
													<div class="item form-group has-feedback">
														<label
															class="text-left col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">建筑:
														</label>
														<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
															<select class="form-control" name="unitCode" disabled="disabled">
																<div th:each="build:${builds.data}">
																	<option th:value="${build.plcBuildCode}" th:text="${build.buildName}"
																		th:selected="${build.plcBuildCode} == ${user.plcBuildCode}"></option>
																</div>
															</select>
														</div>
													</div>
												</div>
											</div>
										</div>
									</div>
								</div>
								<div class="form_content">
									<h4 class="font_ioc_blue">
										<strong><i class="fa fa-gear"></i>用户信息</strong>
									</h4>
									<div class="row">
										<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">创建日期:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="createDate" type="text"
															th:value="${user.createDate}" disabled="disabled">
														<span class="input-group-addon"> <span
															class="glyphicon glyphicon-calendar"></span> </span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">用户名:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="username" placeholder="用户名" type="text"
														th:value="${user.username}" disabled="disabled">
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">出生日期:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="birthDate" type="text"
															th:value="${user.birthDate}" disabled="disabled">
														<span class="input-group-addon"><span
															class="glyphicon glyphicon-calendar"></span> </span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">本次登录时间:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="thisTimeLoginDate" type="text" th:value="--" disabled="disabled"
															th:if="${user.thisTimeLoginDate == null or user.thisTimeLoginDate =='null' or #lists.isEmpty(user.thisTimeLoginDate)}"> 
														<input class="form-control" name="thisTimeLoginDate" type="text" th:value="${user.thisTimeLoginDate}" disabled="disabled"
															th:if="${user.thisTimeLoginDate !=null  and user.thisTimeLoginDate !='null' and  not #lists.isEmpty(user.thisTimeLoginDate)}"> 
														<span class="input-group-addon"> 
															<span class="glyphicon glyphicon-calendar"></span> 
														</span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">更新日期:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="updateDate" type="text" th:value="--" disabled="disabled"
															th:if="${user.updateDate == null or user.updateDate =='null' or #lists.isEmpty(user.updateDate)}"> 
														<input class="form-control" name="updateDate" type="text" th:value="${user.updateDate}" disabled="disabled"
															th:if="${user.updateDate !=null  and user.updateDate !='null' and  not #lists.isEmpty(user.updateDate)}"> 
														<span class="input-group-addon"> <span
															class="glyphicon glyphicon-calendar"></span> </span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">昵称:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="nickname" placeholder="昵称" type="text"
														th:value="${user.nickname}" disabled="disabled">
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">移动电话:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="mobileTelephone" placeholder="移动电话" type="text"
														th:value="${user.mobileTelephone}" disabled="disabled">
												</div>
											</div>
										</div>
										<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">有效时间:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="validDate" type="text"
															th:value="${user.validDate}" disabled="disabled">
														<span class="input-group-addon"> <span
															class="glyphicon glyphicon-calendar"></span> </span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">性别:
												</label>
												<div th:switch="${user.sex}">
													<div th:case="1"
														class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
														<div class="radio radio-inline radio-primary">
															<input type="radio" name="sex" checked="checked"
																disabled="disabled"><label>男</label>
														</div>
														<div class="radio radio-inline radio-primary">
															<input type="radio" name="sex" disabled="disabled"><label>女</label>
														</div>
													</div>
													<div th:case="2"
														class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
														<div class="radio radio-inline radio-primary">
															<input type="radio" name="sex" disabled="disabled"><label>男</label>
														</div>
														<div class="radio radio-inline radio-primary">
															<input type="radio" name="sex" checked="checked"
																disabled="disabled"><label>女</label>
														</div>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">登录次数:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="logonCount" placeholder="登录次数" type="text"
														th:value="${user.logonCount}" disabled="disabled">
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">上次登录时间:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<div class="input-group date">
														<input class="form-control" name="lastTimeLoginDate" type="text" th:value="--" disabled="disabled"
															th:if="${user.lastTimeLoginDate == null or user.lastTimeLoginDate =='null' or #lists.isEmpty(user.lastTimeLoginDate)}"> 
														<input class="form-control" name="lastTimeLoginDate" type="text" th:value="${user.thisTimeLoginDate}" disabled="disabled"
															th:if="${user.lastTimeLoginDate !=null  and user.lastTimeLoginDate !='null' and  not #lists.isEmpty(user.lastTimeLoginDate)}"> 
														<span
															class="input-group-addon"> <span
															class="glyphicon glyphicon-calendar"></span> </span>
													</div>
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">密码:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12"
														name="password" placeholder="密码" type="password"
														th:value="${user.password}" disabled="disabled">
												</div>
											</div>
											<div class="item form-group has-feedback">
												<label
													class="text-left  col-lg-4 col-md-4 col-sm-4 col-xs-12 control-label">电子邮箱:
												</label>
												<div class="col-lg-8  col-md-8 col-sm-8 col-xs-12">
													<input class="form-control col-md-7 col-xs-12" name="email"
														placeholder="电子邮箱" type="text" th:value="${user.email}"
														disabled="disabled">
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
	</div>

</body>
<!-- jQuery -->
<script src="../../js/jquery-1.11.1.min.js"></script>
<!-- 手机端-->
<script src="../../js/jquery.dcjqaccordion.2.7.js"></script>

<!-- Bootstrap -->
<script src="../../js/bootstrap.min.js"></script>
<!-- 滚动条-->
<script src="../../js/jquery.nicescroll.js"></script>
<script src="../../js/jquery.scrollTo.min.js"></script>
<!-- 消息提示 -->
<script type="text/javascript" src="../../js/toastr.js"></script>
<!-- 自定义  -->
<script type="text/javascript" src="../../js/style.js"></script>
<!-- 页面加载 -->
<script src="../../js/nprogress.js"></script>
<!-- 日期 -->
<script src="../../js/bootstrap-datepicker.min.js"
	type="text/javascript"></script>
<script src="../../js/bootstrap-datepicker.zh-CN.min.js"
	type="text/javascript"></script>
<!-- 表单验证 -->
<script src="../../js/bootstrapValidator.min.js"></script>
<script src="../../js/Validator.js"></script>
<script src="../../js/user/user_details.js"></script>
</html>