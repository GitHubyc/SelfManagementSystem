<!DOCTYPE html>
<html xmlns:th="http://www.thymeleaf.org">
<head>
<meta charset="UTF-8"></meta>
<meta http-equiv="X-UA-Compatible" content="IE=edge,Chrome=1" />
<meta name="viewport"
	content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<title>管理责任</title>
<!--[if IE 7]>
    <link href="../css/font-awesome-ie7.min.css" rel="stylesheet">
   <![endif]-->
<!-- Bootstrap -->
<link href="../css/bootstrap.min.css" rel="stylesheet">
<!-- 字体图标 -->
<link href="../css/font-awesome.min.css" rel="stylesheet">
<!-- NProgress -->
<link href="../css/nprogress.css" rel="stylesheet">
<!-- toastr -->
<link href="../css/toastr.min.css" rel="stylesheet">
<!-- 图片上传 -->
<link href="../css/bootstrap-fileinput.css" rel="stylesheet" />
<!-- 公共样式 -->
<link href="../css/general.css" rel="stylesheet" media="screen">
<!-- 选框样式 -->
<link rel="stylesheet" type="text/css" href="../css/build.css">
<!-- 自定义样式 -->
<link href="../css/list.css" rel="stylesheet" media="screen">
<link href="../css/menu.css" rel="stylesheet" media="screen">
</head>
<body>
	<!-- 头部 -->
	<div th:replace="fragments/header_archives :: header">头部</div>

	<div class="content_x">
		<!-- 左菜单 -->
		<div th:replace="fragments/menu_archives :: menu">左菜单</div>

		<!--右内容 -->
		<div class="right_col">
			<div class="padding_20">
				<div class="row">
					<div class="col-md-12 text-center">
						<div class="s_panel text-left">
							<!-- 内容标题 -->
							<div class="x_top clearfix">
							<!-- 	<h5 class="font_blue">主体责任签订</h5> -->
								<i class="fa fa-angle-left font_ioc_blue"></i><a
										href="/build/list">返回上一层</a>
							</div>
							<!-- 内容标题结束 -->
							<div class="padding_15">
							<div class="row">
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
							
							<div class="btn-file">
                       		<div class="file_upload fa-plus-shouye"><img src="" style=" width: 100%; height: 100%;"/></div>
                          						<input type="file" name="buildImage" id="updateimg"
													onchange="ajaxFileUpload('updateimg','pic','fileimgid')"
													accept="image/gif,image/jpeg,image/x-png" /></div>
                        	
							</div>
							
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
							
							<div class="btn-file">
                       		<div class="file_upload fa-plus-weiye"><img id="pic1" src="" style="width: 100%; height: 100%;"/></div>
                           <input type="file" name="buildImage" id="updateimg1"
													onchange="ajaxFileUpload('updateimg1','pic1','fileimgid1')"
													accept="image/gif,image/jpeg,image/x-png" /></div>
                        	</div>
                        	
							</div>
							</div>
							
							
							</div>

						</div>
					</div>
				</div>
			</div>

		</div>
	</div>
		
</body>
<script th:replace="fragments/configjs :: js" type="text/javascript"></script>

</html>