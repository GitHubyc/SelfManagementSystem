<!DOCTYPE html>
<html lang="zh-CN">
<head>
<meta charset="utf-8">
<meta http-equiv="X-UA-Compatible" content="IE=edge,Chrome=1" />
<meta name="viewport"
	content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
<title>传感器信息</title>
<!--[if IE 7]>
    <link href="../css/font-awesome-ie7.min.css" rel="stylesheet">
   <![endif]-->
<!-- Bootstrap -->
<link href="../css/bootstrap.min.css" rel="stylesheet">
<!-- 字体图标 -->
<link href="../css/font-awesome.min.css" rel="stylesheet">
<!-- NProgress -->
<link href="../css/nprogress.css" rel="stylesheet">
<!-- 日期样式 -->
<link href="../css/bootstrap-datepicker3.min.css" rel="stylesheet">
<!-- 公共样式 -->
<link href="../css/general.css" rel="stylesheet" media="screen">
<!-- 消息提示 -->
<link href="../css/toastr.min.css" rel="stylesheet">
<!-- 文本验证 -->
<link href="../css/bootstrapValidator.min.css" rel="stylesheet">
<!-- 选框样式 -->
<link rel="stylesheet" type="text/css" href="../css/build.css">
<!-- 图片上传 -->
<link href="../css/bootstrap-fileinput.css" rel="stylesheet" />
<link href="../css/add.css" rel="stylesheet" media="screen">


</head>
<body>
	<!-- 头部 -->
	<div th:replace="fragments/header :: header">头部</div>

	<div class="content_x">
		<!-- 左菜单 -->
		<div th:replace="fragments/menu :: menu">左菜单</div>

		<!--右内容 -->
		<div class="right_col">
			<div class="padding_20">
				<div class="row">
					<div class="col-md-12">
						<div class="x_panel">
							<div class="x_top clearfix">
								<div class="font-left">
									<i class="fa fa-angle-left font_ioc_blue"></i><a
										th:href="@{/firecontrolinstrumentation/list}">返回传感器列表</a>
								</div>

							</div>
							<div class="x_top x_top1 clearfix">
								<button type="button" class="btn btn-blue  btn-color">前置机</button>
								<button type="button" class="btn btn-blue ">传感器</button>
								<button type="button" class="btn btn-blue ">消防主机</button>
								<button type="button" class="btn btn-blue ">探点</button>
							</div>

							<div class="x_content" id="front_end_computer"></div>
							<div class="x_content displaynone" id="sensor"></div>
							<div class="x_content displaynone" id="fire_engine"></div>
							<div class="x_content displaynone" id="spot_point"></div>
						</div>
					</div>
				</div>
			</div>

		</div>

	</div>
</body>

<!-- jQuery -->
<script src="../js/jquery-1.11.1.min.js"></script>
<!-- 手机端-->
<script src="../js/jquery.dcjqaccordion.2.7.js"></script>

<!-- Bootstrap -->
<script src="../js/bootstrap.min.js"></script>
<!-- 滚动条-->
<script src="../js/jquery.nicescroll.js"></script>
<script src="../js/jquery.scrollTo.min.js"></script>
<!-- 日期 -->
<script src="../js/bootstrap-datepicker.min.js" type="text/javascript"></script>
<script src="../js/bootstrap-datepicker.zh-CN.min.js"
	type="text/javascript"></script>
<!-- 消息提示 -->
<script type="text/javascript" src="../js/toastr.js"></script>
<!-- 自定义  -->
<script src="../js/style.js"></script>
<!-- 弹窗框  -->
<script type="text/javascript" th:src="@{../js/layer.js}"></script>
<script src="../js/message.js"></script>
<!-- 页面加载 -->
<script src="../js/nprogress.js"></script>
<!-- 表单验证 -->
<script src="../js/bootstrapValidator.min.js"></script>
<script src="../js/Validator.js"></script>
<!-- 图片上传-->
<script src="../js/ajaxfileupload.js" type="text/javascript"></script>
<script src="../js/FileUpload.js" type="text/javascript"></script>
<script
	src="../js/FireControlInstrumentation/FireControlInstrumentation_add.js"
	type="text/javascript"></script>
<script type="text/javascript">
	//日期插件
	$('.date').datepicker({
		format : "yyyy-mm-dd",
		language : "zh-CN",
		todayBtn : "linked",
		clearBtn : true,
	});
</script>
</html>