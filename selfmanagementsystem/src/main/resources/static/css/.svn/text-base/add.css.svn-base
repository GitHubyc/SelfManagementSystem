.col-lg-3{
	width:25%\9;
	float:left;
}
.col-lg-9 {
    width: 66.66666667%\9;
	float:left;
}
.col-lg-2 {
    width:110px\9;
	height:34px\9;
}
.col-lg-10 {
    width: 80%\9;
}
.x_top1 h4{
	margin-top:20px;
	color: #666;
}

.x_top1 .btn,.x_top2 .btn{
    padding: 6px 24px;
    *padding-top:9px;
}
.col_left_menu{
width: 20%;
    float: left;
    min-height: 720px;
    border: 1px solid #ececec;
    min-width: 130px;
    box-shadow: 0px 0px 5px #888888;
    padding-bottom:20px;
    margin-left: 3px;
    margin-bottom: 3px;
}
.col_left_menu ul,.tz_left ul,.fj_right ul,.search_box ul,.tab-content-yjc{
    list-style-type:none;
    padding:0;
    margin:0;

}
.col_left_menu h5{
    background-color: #f5f5f5;
    margin: 0;
    padding: 10px;
    border-bottom: 1px solid #e7eaed;
}

.col_left_menu h5 a{
	float:right;
}
.lc_title_tab{
    display: inline;
    float: right;
    *margin-top: -25px;
    *margin-right: 15px;
}
.lc_title_tab span{
    margin-left: 5px;
    cursor: pointer;
}
.text-c{
    margin-top:10px;
    padding-left:10px;
    clear: both;
}
.list_tb li{
    padding: 10px 5px;
    text-align: center;
    border-bottom: 1px solid #ececec;
    cursor: pointer;
}
.list_col_c li:hover,.list_col_c .bg_blue{
    color: white;
    background: #3385ff;
}
.list_tb li:hover,.bg_blue{
    color: white;
    background: #3385ff;
    margin: 0 -1px;
}
.list_col_c li{
    width: 40px;
    height: 40px;
    background: #ecf0f5;
    border-radius: 50%;
    font-size: 14px;
    text-align: center;
    line-height: 40px;
    cursor: pointer;
    display: inline-block;
    margin: 9px 9px;
    *margin: 9px 11px;
    *float: left;
    position: relative;
}
.col_right_tb{
    width: 78%;
    float: right;
}
.table>tbody>tr>td, .table>tbody>tr>th, .table>tfoot>tr>td, .table>tfoot>tr>th, .table>thead>tr>td, .table>thead>tr>th {
    vertical-align: middle;
}
.content_row{
    display: none;
}
.title_top{
     text-align: left;
    padding-bottom: 10px;
 }
.title_top a{
    float: right;
    *margin-top: -35px;
}
.search_box h5 a{
 color: black;
    float: right;
    *margin-top: -15px;
}
.search_box ul li{
    color: #959595;
    overflow: hidden;
    width: 100%;
    text-overflow:ellipsis;
    white-space:nowrap;
    padding: 5px 0;
}
.search_box{
    width: 100%;
    margin-bottom: 20px;
    display: none;
}
.search_box ul{
    float: left;
    padding:0 1.5%;
    *padding:0 0.9%;
    width: 20%;
    *width: 18%;
    font-size: 12px;
}
.search_box_line{
    border: 1px solid #ececec;
    padding:10px;

}
#column1 p span{
 color: #ccc;
 font-size: 14px;
}
.tz_left{
width: 100%;
    margin-bottom: 15px;
}
.tz_left ul li{
    border: 1px solid #ececec;
    text-align: center;
    padding:15px 15px 16px;
    margin-left: -1px;
     width: 33.333%;
     float: left;
}
.inline{
    display: inline;
}
.tz_left ul li .Img{
    width: 100%;
    height:200px;
}
.fj_right{
    width: 100%;
      margin-bottom: 10px;
}
.col_right_tb .fj_right ul,.search_box_row{
    margin: 0 -0.5%;
}

.fj_right ul li{
    padding:0 5px;
    width: 33.333%;
    *width: 28%; 
    float: left;
  
}
.floor_line{

 border: 1px solid #ececec;
 margin-bottom: 10px;
   min-height: 180px;
    
}
.text_box{
    width: 100%;
    height:80px;
	display: -webkit-box;
-webkit-box-orient: vertical;
-webkit-line-clamp: 4;
overflow: hidden;

}
.fj_right ul li i{
    text-align: center;
    display: block;
    color: #959595;
    line-height: 11rem;
}

.fj_right ul li h5{
    border-bottom: 1px solid #ececec;
    margin: 0 0 10px 0;
    padding: 10px;
}
.fj_right ul li p{
    padding:0 10px;
    margin-bottom: 0;
    color: #959595;
    width: 100%;

}
.fj_right ul li p span,.search_box  ul li span,.box_content span{
    color: #000;

}
.fj_right ul li p .font_blue,.search_box .font_blue {
    color: #3385ff;
}
.fj_right ul li h5 a{;
    padding:0;
    *margin-top:-25px;
    *padding-right: 10px;
}
.fj_right ul li a{
    float: right;
    padding-right: 10px;
}

.content_row{
    *width: 97%;
}

.tab-content-lc,.tab-content-lcxx,.displaynone{
    display: none;
}





@media (min-width: 768px){
	
    .col-md-offset-3 {
        margin-left: 17%;
    }

}

@media (max-width: 1024px){
    .search_box ul{
        width: 50%;
        font-size: 14px;
    }
    .fj_right ul li{
        width: 100%;
    }
    .box_margin{
        width: 50%;
    }

    .list_col_c li{
        margin: 9px 10%;
    }
    .x_scrollbar{
        min-width: 720px;
    }
    .col_left_menu{
        min-height:0px;
    }
    .col_left_menu h5 span{
    display: none;
    }
}

@media (max-width: 768px){
	.font_12{
	font-size: 12px;
	}
    .list_col_c li{
        margin: 9px 8%;
    }

    .x_scrollbar{
        min-width: 653px;
    }

}
